import java.util.Arrays;
public class Sorts{
    public static void main(String[]args){
	int[] test = new int[6];
	test[0] = 1;
	test[1] = 2;
	test[2] = 9;
	test[3] = 5;
	test[4] = 0;
	test[5] = 3;
	if(args[0] == "selection"){
	    selectionSort(test);
	}
    }
    
    public static void selectionSort(int[] data){
	int min = data[0];
	for(int i = 0; i < data.length; i++){
	    int counter = 0;
	    while(counter < data.length){
		if(data[counter] < min){
		    min = data[counter];
		}
	    }
	    data[counter] == data[i];
	    data[i] = min;
	    System.out.println(data.toString());
	}
    }
}